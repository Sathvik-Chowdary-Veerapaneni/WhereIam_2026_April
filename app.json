{
  "expo": {
    "name": "Debt Mirror",
    "slug": "debt-mirror",
    "version": "1.0.0",
    "scheme": "debtmirror",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTabletMode": true,
      "bundleIdentifier": "com.debtmirror.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.debtmirror.app"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      [
        "expo-secure-store",
        {}
      ]
    ]
  }
}